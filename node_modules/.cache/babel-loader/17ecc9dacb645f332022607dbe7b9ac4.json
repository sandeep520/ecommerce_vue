{"ast":null,"code":"import Vue from \"vue\";\nimport axios from \"axios\";\nimport VueAxios from \"vue-axios\";\nimport router from \"@/router\";\nimport Cart from \"./Cart.vue\";\nimport Header from \"@/Header/Header.vue\";\nVue.use(VueAxios, axios);\nexport default {\n  name: \"Produts\",\n\n  data() {\n    return {\n      page: \"products\",\n      list: [],\n      cart: [],\n      search: \"\",\n      loading: false,\n      totalCart: 0\n    };\n  },\n\n  mounted() {\n    this.getProduct();\n  },\n\n  methods: {\n    getProduct() {\n      this.loading = !false;\n      this.axios.get(\"https://fakestoreapi.com/products\").then(res => {\n        this.list = res.data;\n        setTimeout(() => {\n          this.loading = !true;\n        }, 1000);\n      });\n    },\n\n    addProduct(item) {\n      this.cart.push(item);\n      localStorage.setItem(\"cart\", JSON.stringify(this.cart));\n      router.push('/productDetails/' + item.id);\n    },\n\n    addToCart(item) {\n      this.cart.push(item);\n      router.push(\"/cart/\" + item.id);\n    }\n\n  },\n  computed: {\n    filterData() {\n      return this.list.filter(searchData => searchData.category.includes(this.search));\n    }\n\n  },\n  components: {\n    Cart,\n    Header\n  }\n};","map":{"version":3,"mappings":"AAmEA;AACA;AACA;AACA;AACA;AACA;AACAA;AACA;EACAC,eADA;;EAEAC;IACA;MACAC,gBADA;MAEAC,QAFA;MAGAC,QAHA;MAIAC,UAJA;MAKAC,cALA;MAMAC;IANA;EAQA,CAXA;;EAYAC;IACA;EACA,CAdA;;EAgBAC;IACAC;MACA;MACA;QACA;QACAC;UACA;QACA,CAFA,EAEA,IAFA;MAGA,CALA;IAMA,CATA;;IAWAC;MACA;MACAC;MACAC;IACA,CAfA;;IAiBAC;MACA;MACAD;IACA;;EApBA,CAhBA;EAsCAE;IACAC;MACA,sCACAC,yCADA;IAGA;;EALA,CAtCA;EA6CAC;IAAAC;IAAAC;EAAA;AA7CA","names":["Vue","name","data","page","list","cart","search","loading","totalCart","mounted","methods","getProduct","setTimeout","addProduct","localStorage","router","addToCart","computed","filterData","searchData","components","Cart","Header"],"sourceRoot":"src/components","sources":["Products.vue"],"sourcesContent":["<template>\n\n  <div class=\"container\">\n    <div class=\"main-product mt-4 text-monospace\">\n      <h1>Products Details...</h1>\n\n      <div v-if=\"loading\" class=\"spinner-border m-5\" role=\"status\">\n        <span class=\"sr-only\">Loading...</span>\n      </div>\n      <div>\n        <div class=\"search input-group\">\n          <input\n            type=\"search\"\n            v-model=\"search\"\n            class=\"form-control rounded\"\n            placeholder=\"Search\"\n            aria-label=\"Search\"\n            aria-describedby=\"search-addon\"\n          />\n        </div>\n        <button class=\"cart-btn btn btn-primary\">\n          cart:{{ cart.length }}\n        </button>\n      </div>\n    </div>\n\n    <div class=\"row\">\n      <div\n        v-for=\"item of filterData\"\n        v-bind:key=\"item.id\"\n        class=\"col-md-6 card\"\n        style=\"width: 18rem\"\n      >\n        <!-- list does not filter -->\n\n        <router-link :to=\"'/productDetails/' + item.id\">\n          <img\n            style=\"height: 273px\"\n            class=\"card-img-top\"\n            v-bind:src=\"item.image\"\n            alt=\"Card image cap\"\n          />\n        </router-link>\n\n        <div class=\"card-body col-md-12\">\n          <h5 class=\"card-title\">Product Name: {{ item.title }}</h5>\n          <h5 class=\"card-title\">Price: {{ item.price }}</h5>\n          <h5 class=\"card-title\">Description:</h5>\n          <h5 class=\"card-title\">Categories: {{ item.category }}</h5>\n          <button\n            v-on:click=\"addProduct(item)\"\n            style=\"margin-right: 15px\"\n            class=\"btn btn-primary\"\n          >\n            View Product....\n          </button>\n        </div>\n      </div>\n    </div>\n    <!-- <Cart :addCart='cart'/> -->\n    \n\n</div>\n\n</template>\n\n<script>\nimport Vue from \"vue\";\nimport axios from \"axios\";\nimport VueAxios from \"vue-axios\";\nimport router from \"@/router\";\nimport Cart from \"./Cart.vue\";\nimport Header from \"@/Header/Header.vue\";\nVue.use(VueAxios, axios);\nexport default {\n  name: \"Produts\",\n  data() {\n    return {\n      page: \"products\",\n      list: [],\n      cart: [],\n      search: \"\",\n      loading: false,\n      totalCart:0\n    };\n  },\n  mounted() {\n    this.getProduct();\n  },\n\n  methods: {\n    getProduct() {\n      this.loading = !false;\n      this.axios.get(\"https://fakestoreapi.com/products\").then((res) => {\n        this.list = res.data;\n        setTimeout(() => {\n          this.loading = !true;\n        }, 1000);\n      });\n    },\n\n    addProduct(item) {\n      this.cart.push(item);\n      localStorage.setItem(\"cart\", JSON.stringify(this.cart));\n      router.push('/productDetails/'+ item.id)\n    },\n\n    addToCart(item) {\n      this.cart.push(item);\n      router.push(\"/cart/\" + item.id);\n    },\n  },\n  computed: {\n    filterData() {\n      return this.list.filter((searchData) =>\n        searchData.category.includes(this.search)\n      );\n    },\n  },\n  components: { Cart, Header },\n};\n</script>\n\n<style scoped>\n.main-product {\n  text-decoration: underline;\n}\n.card {\n  margin-left: 40px;\n  margin: 18px;\n  border-radius: 20px;\n}\n.card-main {\n  display: inline-flex !important;\n}\n.cart-btn {\n  margin-left: 80%;\n}\n.search {\n  width: 20%;\n  margin: 0 60%;\n  margin-bottom: -38px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}