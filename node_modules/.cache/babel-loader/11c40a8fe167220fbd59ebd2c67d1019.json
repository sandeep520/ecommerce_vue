{"ast":null,"code":"import Vue from \"vue\";\nimport axios from \"axios\";\nimport VueAxios from \"vue-axios\";\nimport router from \"@/router\";\nimport Cart from \"./Cart.vue\";\nimport Header from \"@/Header/Header.vue\";\nimport Banner from \"./Banner.vue\";\nimport Pagination from \"./Pagination.vue\";\nimport NewsLetter from \"./NewsLetter.vue\";\nVue.use(VueAxios, axios);\nexport default {\n  name: \"Produts\",\n\n  data() {\n    return {\n      page: \"products\",\n      list: [],\n      cart: [],\n      search: \"\",\n      loading: false,\n      totalCart: 0,\n      covid: []\n    };\n  },\n\n  mounted() {\n    this.getProduct();\n    this.totolCart();\n    this.getCovid();\n  },\n\n  methods: {\n    getProduct() {\n      this.loading = !false;\n      this.axios.get(\"https://fakestoreapi.com/products\").then(res => {\n        this.list = res.data;\n        console.log();\n\n        if (res.data != \"\") {\n          this.loading = !true;\n        }\n      });\n    },\n\n    addProduct(item) {\n      this.cart.push(item);\n      localStorage.setItem(\"cart\", JSON.stringify(this.cart));\n      router.push(\"/productDetails/\" + item.id);\n    },\n\n    totolCart(item) {\n      let cart = JSON.parse(localStorage.getItem(\"cart\"));\n      console.log(cart);\n      this.totalCart = cart != null && cart.length > 0 ? cart.length : 0;\n    },\n\n    getCovid() {\n      this.axios.get(\"https://api.covid19api.com/summary\").then(res => {\n        console.log(res.data);\n        this.covid.push(res.data);\n        console.log(this.covid);\n      });\n    }\n\n  },\n  computed: {\n    filterData() {\n      return this.list.filter(searchData => searchData.category.includes(this.search));\n    }\n\n  },\n  components: {\n    Cart,\n    Header,\n    Banner,\n    Pagination,\n    NewsLetter\n  }\n};","map":{"version":3,"mappings":"AAuEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACAA;AACA;EACAC,eADA;;EAEAC;IACA;MACAC,gBADA;MAEAC,QAFA;MAGAC,QAHA;MAIAC,UAJA;MAKAC,cALA;MAMAC,YANA;MAOAC;IAPA;EASA,CAZA;;EAcAC;IACA;IACA;IACA;EACA,CAlBA;;EAoBAC;IACAC;MACA;MACA;QACA;QACAC;;QACA;UACA;QACA;MACA,CANA;IAOA,CAVA;;IAYAC;MACA;MACAC;MACAC;IACA,CAhBA;;IAkBAC;MACA;MACAJ;MACA;IAEA,CAvBA;;IAyBAK;MACA;QACAL;QACA;QACAA;MAEA,CALA;IAMA;;EAhCA,CApBA;EAuDAM;IACAC;MACA,sCACAC,yCADA;IAGA;;EALA,CAvDA;EA8DAC;IAAAC;IAAAC;IAAAC;IAAAC;IAAAC;EAAA;AA9DA","names":["Vue","name","data","page","list","cart","search","loading","totalCart","covid","mounted","methods","getProduct","console","addProduct","localStorage","router","totolCart","getCovid","computed","filterData","searchData","components","Cart","Header","Banner","Pagination","NewsLetter"],"sourceRoot":"src/components","sources":["Products.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"main-product mt-4 text-monospace\">\n      <h1>Products Details...</h1>\n      <div v-if=\"loading\" class=\"spinner-border m-5\" role=\"status\">\n        <span class=\"sr-only\">Loading...</span>\n      </div>\n      <div>\n        <div class=\"search input-group\">\n          <input\n            type=\"search\"\n            v-model=\"search\"\n            class=\"form-control rounded\"\n            placeholder=\"Search\"\n            aria-label=\"Search\"\n            aria-describedby=\"search-addon\"\n          />\n        </div>\n        <button class=\"cart-btn btn btn-primary\">\n          cart:{{ totalCart }}\n        </button>\n      </div>\n    </div>\n\n    <div class=\"row justify-content-center mt-4\">\n      <div\n        v-for=\"item of filterData\"\n        v-bind:key=\"item.id\"\n        class=\"col-12 card\"\n        style=\"width: 15rem\"\n      >\n        <!-- list does not filter -->\n\n        <router-link :to=\"'/productDetails/' + item.id\">\n          <img\n            style=\"height: 200px\"\n            class=\"card-img-top\"\n            v-bind:src=\"item.image\"\n            alt=\"Card image cap\"\n          />\n        </router-link>\n\n        <div class=\"card-body col-md-12\">\n          <h5 class=\"card-title\">\n            Product Name: {{ item.title.slice(0, 15) + \"...\" }}\n          </h5>\n          <h5 class=\"card-title\">Price: {{ item.price }}</h5>\n          <h5 class=\"card-title\">Categories: {{ item.category }}</h5>\n          <button\n            v-on:click=\"addProduct(item)\"\n            style=\"margin-right: 15px\"\n            class=\"btn btn-primary\"\n          >\n            View Product....\n          </button>\n        </div>\n      </div>\n    </div>\n    <Pagination />\n    <NewsLetter />\n    <Banner />\n\n    <select :v-for=\"data in covid\" class=\"drop mx-2\" >\n      <option value=\"volvo\">{asass}</option>\n      <option value=\"logout\">Logout</option>\n    </select>\n    <!-- <Cart :addCart='cart'/> -->\n  </div>\n</template>\n\n<script>\nimport Vue from \"vue\";\nimport axios from \"axios\";\nimport VueAxios from \"vue-axios\";\nimport router from \"@/router\";\nimport Cart from \"./Cart.vue\";\nimport Header from \"@/Header/Header.vue\";\nimport Banner from \"./Banner.vue\";\nimport Pagination from \"./Pagination.vue\";\nimport NewsLetter from \"./NewsLetter.vue\";\nVue.use(VueAxios, axios);\nexport default {\n  name: \"Produts\",\n  data() {\n    return {\n      page: \"products\",\n      list: [],\n      cart: [],\n      search: \"\",\n      loading: false,\n      totalCart: 0,\n      covid:[],\n    };\n  },\n\n  mounted() {\n    this.getProduct();\n    this.totolCart();\n    this.getCovid()\n  },\n\n  methods: {\n    getProduct() {\n      this.loading = !false;\n      this.axios.get(\"https://fakestoreapi.com/products\").then((res) => {\n        this.list = res.data;\n        console.log();\n        if (res.data != \"\") {\n          this.loading = !true;\n        }\n      });\n    },\n\n    addProduct(item) {\n      this.cart.push(item);\n      localStorage.setItem(\"cart\", JSON.stringify(this.cart));\n      router.push(\"/productDetails/\" + item.id);\n    },\n\n    totolCart(item) {\n      let cart = JSON.parse(localStorage.getItem(\"cart\"));\n      console.log(cart);\n      this.totalCart = cart != null && cart.length > 0 ? cart.length : 0;\n      \n    },\n\n    getCovid() {\n      this.axios.get(\"https://api.covid19api.com/summary\").then((res) => {\n        console.log(res.data);\n        this.covid.push(res.data)\n        console.log(this.covid)\n       \n      });\n    },\n   \n  },\n  computed: {\n    filterData() {\n      return this.list.filter((searchData) =>\n        searchData.category.includes(this.search)\n      );\n    },\n  },\n  components: { Cart, Header, Banner, Pagination, NewsLetter },\n};\n</script>\n\n<style scoped>\n.main-product {\n  text-decoration: underline;\n}\n.card {\n  margin: 5px 10px;\n  border-radius: 20px;\n}\n.card-main {\n  display: inline-flex !important;\n}\n.cart-btn {\n  margin-left: 80%;\n}\n.search {\n  width: 20%;\n  margin: 0 60%;\n  margin-bottom: -38px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}